{
  "name": "tradie",
  "version": "5.3.2",
  "description": "A CLI for building web apps. Like create-react-app but configurable via templates.",
  "keywords": [
    "webpack",
    "hot-reloading",
    "jest",
    "build",
    "test",
    "nwb",
    "create-react-app",
    "crap"
  ],
  "repository": "jameslnewell/tradie-v4",
  "files": [
    "lib"
  ],
  "bin": {
    "tradie": "lib/tradie.js"
  },
  "dependencies": {
